<br>
<div class="container">
	<%@orders.each do |order|%>
		<div class="row">
			<div class="col-sm-1"></div>
			<div class="col-sm-10 well" id="content">
			<p>Order Id : <%= order.id%></p>
			<p>Order Created at : <%= order.created_at %></p>
			<%= link_to "View Details",orders_view_path(:id=>order.id),:method=>:get,:class=>"btn btn-primary"%>
			</div>
			<div class="col-sm-1"></div>
		</div>
	<%end%>
</div>